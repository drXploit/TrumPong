          <argument>
            <kind>1</kind>
            <string>///Enemy step code for collision
lerpX = lerp(lerpX,image_xscale,0.1);

lerpY = lerp(lerpY,image_yscale,0.1);

if (sprite_index == spr_huge)
{
    speed = 1;                          // temporarily slow down
    if(instance_exists(obj_player))
    {
        obj_player.canFire = false;         // stop player from shooting
        obj_player.alarm[0] = room_speed;   // player alarm for shooting
        if (instance_exists(obj_bullet))    // stop all the bullets for this animation
        {
            with (obj_bullet)
            {
                speed = 0;
            }
        }
    }
    if (instance_exists(obj_knife_hand))
    {
        obj_knife_hand.canFire = false;
        obj_knife_hand.alarm[0] = room_speed;
        if (instance_exists(obj_knife))
        {
            with (obj_knife)
            {
                speed = 0;
            }
        }
    }
    audio_pause_all();
    drawTextAllow = true;  // Draw "verbal" text on screen
}
else
{
    speed = adjustedSpeed;                 // normal speed

    if (instance_exists(obj_bullet))
    {
        with (obj_bullet)
        {
            speed = bulletSpeed;        // normal bullet speeds
        }
    }
    if (instance_exists(obj_knife))
    {
        with (obj_knife)
        {
        speed = bulletSpeed;
        }
    }
    audio_resume_all();
    drawTextAllow = false;              // dont draw "verbal" text on screen
}

tmpV = vspeed;   // to see if enemy bounced

move_bounce_solid(false);    // Bounce off the solid walls

if (tmpV != vspeed)          // The enemy bounced so squeeze head
{
    audio_play_sound(snd_light_thud,9,false);
    lerpX *= 1.2;
    lerpY *= 0.75;
}

</string>
